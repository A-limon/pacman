<div class="clearfix">
	<div id="logo">
		<a href="<%- config.root %>"><img src="/img/logo.svg" alt="Alimon"/></a>
		<hgroup>
			<h1 class="site-name"><a href="<%- config.root %>"><%= config.title %></a></h1>
			<h2 class="blog-motto"><% if (config.subtitle){ %><%= config.subtitle %><% } %></h2>
		</hgroup>
	 </div>
	 <div class="navbar"><a class="navbutton" href="#"><i class="icon-menu"></i></a></div>
	<nav>
		<ul>
		 <% for (var i in theme.menu){ %>
			<li><a href="<%- theme.menu[i] %>"><%= i %></a></li>
		<% } %>
			<li><form class="search" action="//google.com/search" method="get" accept-charset="utf-8">
				<label>Search</label>
					<input type="search" name="q" id="search" autocomplete="off" autocorrect="off" autocapitalize="off" maxlength="20" placeholder="Search" />
					<input type="hidden" name="q" value="site:<%- config.url.replace(/^https?:\/\//, '') %>">
				</form>
			</li>
		</ul>
	</nav>
</div>